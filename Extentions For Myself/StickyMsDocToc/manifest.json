{

    "manifest_version": 2,
    "name": "stickyMsDocToc",
    "version": "1.0",

    "description": "Adds a red border to all webpages matching mozilla.org.",

    "icons": {
        "48": "icons/border-48.png"
    },

    "content_scripts": [{
        "matches": ["*://docs.microsoft.com/*"],
        "js": ["stickyMsDocToc.js"]
    }]

}
